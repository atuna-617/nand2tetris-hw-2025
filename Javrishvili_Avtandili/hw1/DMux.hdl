/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT out1, out2;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=in, b=notSel, out=out1);
    And(a=in, b=sel, out=out2);
}